{
  "name": "haexhub-sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for HaexHub.",
  "repository": {
    "type": "git",
    "url": "https://github.com/haexhub/haex-hub.git",
    "directory": "haexhub-sdk"
  },
  "keywords": [
    "tauri",
    "sdk",
    "haexhub"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit"
  },
  "author": "haex",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "@types/node": "^24.5.2",
    "eslint": "^9.36.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@tauri-apps/api": "^2.8.0"
  }
}
