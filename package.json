{
  "name": "@haexhub/sdk",
  "version": "0.1.64",
  "description": "SDK for building HaexHub extensions",
  "repository": {
    "type": "git",
    "url": "https://github.com/haexhub/sdk.git",
    "directory": "sdk"
  },
  "keywords": [
    "tauri",
    "sdk",
    "extension",
    "haexhub"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.js"
    },
    "./react": {
      "types": "./dist/react.d.ts",
      "import": "./dist/react.mjs",
      "require": "./dist/react.js"
    },
    "./svelte": {
      "types": "./dist/svelte.d.ts",
      "import": "./dist/svelte.mjs",
      "require": "./dist/svelte.js"
    },
    "./nuxt": {
      "types": "./dist/nuxt.d.ts",
      "import": "./dist/nuxt.mjs",
      "require": "./dist/nuxt.js"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.js"
    }
  },
  "bin": {
    "haexhub": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsup src/index.ts src/cli/index.ts src/vue.ts src/react.ts src/svelte.ts src/nuxt.ts src/vite.ts --format cjs,esm --dts --external @nuxt/kit --external vite --external vue --external react --external svelte --external svelte/store",
    "dev": "tsup --watch",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "update:patch": "pnpm build && pnpm version patch",
    "update:minor": "pnpm build && pnpm version minor",
    "update:major": "pnpm build && pnpm version major",
    "publishVersion": "pnpm publish --access public"
  },
  "author": "haex",
  "license": "ISC",
  "packageManager": "pnpm@10.15.1",
  "devDependencies": {
    "@nuxt/kit": "^4.1.3",
    "@types/archiver": "^6.0.3",
    "@types/node": "^24.5.2",
    "@types/react": "^19.2.2",
    "eslint": "^9.36.0",
    "react": "^19.2.0",
    "svelte": "^5.39.11",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vite": "^7.1.9",
    "vue": "^3.5.22"
  },
  "peerDependencies": {
    "@nuxt/kit": "^3.0.0 || ^4.0.0",
    "react": "^18.0.0 || ^19.0.0",
    "svelte": "^4.0.0 || ^5.0.0",
    "vite": "^5.0.0 || ^6.0.0 || ^7.0.0",
    "vue": "^3.0.0"
  },
  "peerDependenciesMeta": {
    "@nuxt/kit": {
      "optional": true
    },
    "vite": {
      "optional": true
    },
    "vue": {
      "optional": true
    },
    "react": {
      "optional": true
    },
    "svelte": {
      "optional": true
    }
  },
  "dependencies": {
    "archiver": "^7.0.1",
    "commander": "^14.0.1",
    "drizzle-orm": "^0.44.6"
  }
}
